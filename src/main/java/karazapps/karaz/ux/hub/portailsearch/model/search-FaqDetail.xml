                    $(".header-div-toggle").slideToggle(); 
                });         
    
                $(".Header-signup-logo .Header-signup-glo .rokhas-home,.Header-signup .Header-signup-min .menu-bar-img").click(function(){
                    ApplicationManager.run("karaz/ux/hub/portailsearch/search/PublicCentralSearch", "search", "Public central search", {});
                });

                $(".header-div-toggle .dropdown").mouseenter(function(){
                    $(".header-div-toggle .dropdown-content").slideDown();
                }).mouseleave(function(){
                    $(".header-div-toggle .dropdown-content").slideUp();
                });

                intializeFaqPages();
                loadPageBytype(2);
                var id = root.query.idObject;
                //var id = "vBcI22oBLueSVKZn4Ob4";
                getQsFaq(id,1);


                PCD_headerfieldset_color(".NFQ-quest-title");
	            $(".NFQ-quest-title .ow-label-pl").css('text-transform','capitalize');
                PCD_header_style_quest_type(".NFQ-type-esign" , "far fa-edit")
                PCD_header_style_quest_type(".NFQ-type-general" , "far fa-question-circle")
               
                PCD_header_style_quest_type(".NFQ-type-document" , "far fa-file-pdf")
                PCD_header_style_quest_type(".NFQ-type-plat" , "fab fa-simplybuilt")
                PCD_header_style_quest_type(".NFQ-type-archit" , "fas fa-user")
                PCD_header_style_quest_type(".NFQ-type-adminis" , "fas fa-users-cog")


                
                // header color
                PCD_headerfieldset_color(".pcd-ha-color");

                $(".NQF-vue-ref").hide();
                RestSearchFaqSec("",0,5,0,[".NFQ-quest-type-document",".NFQ-quest-type-plat",".NFQ-quest-type-general",".NFQ-quest-type-esign",".NFQ-quest-type-archit",".NFQ-quest-type-adminis"],3);

                // hide saisie info question
                $(".NQF-edit-modif").hide()

            </script>

