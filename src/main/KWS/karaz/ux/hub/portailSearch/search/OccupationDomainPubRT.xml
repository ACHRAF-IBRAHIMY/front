              </html>
      </renderer>
    
      <renderer name="haut">
          <html columns="100%">
              <span class="scroll-top-sp">
                <i class="fas fa-angle-double-left" />
                <span>${code | translate}</span>
              </span>
            </html>
      </renderer>
    
      <renderer name="htmlISpan">
          <html columns="100%">
              <i class="${iClass}" style="${iStyle}" />
              <span style="${spanStyle}">${code | translate}</span>
            </html>
      </renderer>
    
